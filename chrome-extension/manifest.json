{
  "manifest_version": 2,

  "name": "TA Pixel Helper",
  "description": "Helps validate your TripAdvisor Custom Audience pixels.",
  "version": "0.0.1",
  "background": {
    "scripts": ["build/background.min.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*.tripadvisor.com/"
  ]
}
